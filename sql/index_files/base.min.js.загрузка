(function(b,f,e,h){b(e).on("editor:config",function(e,c,f){c.htmlAllowedTags.push("link");try{var a=b.parseJSON(b(".js-klEditorConfig").first().html())||{};for(var d in a)Object.prototype.hasOwnProperty.call(a,d)&&(c[d]=a[d]);Object.assign(c,a)}catch(g){console.error(g)}})})(jQuery,window,document);