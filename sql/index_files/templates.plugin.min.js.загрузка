(function(b){(function(){var d='<ul class="fr-dropdown-list">';try{var e=b.parseJSON(b(".js-klEditorTemplates").first().html())||{}}catch(g){e=!1}b.FE.DefineIcon("klTemplates",{NAME:"clipboard"});b.FE.DefineIcon("klInsertTemplate",{NAME:"file-alt"});for(var f in e){var a=e[f];if(a.templates.length){d+='<li class="klEMTemplateGroupTitle">'+a.title+"</li>";for(var c in a.templates)d+='<li><a class="fr-command" data-cmd="klInsertTemplate-'+f+"-"+c+'">_ICO&nbsp;&nbsp;'+a.templates[c].title+"</a></li>",
function(a,c){b.FE.RegisterCommand(c,{undo:!0,focus:!0,icon:"klTemplates",callback:function(){this.html.insert(a.content)}})}(a.templates[c],"klInsertTemplate-"+f+"-"+c)}}(function(a){b.FE.RegisterCommand("klTemplates",{type:"dropdown",title:"Templates",icon:"klTemplates",undo:!1,focus:!0,html:function(){return a.replace(/_ICO/g,this.icon.create("klInsertTemplate"))}})})(d+"</ul>")})()})(jQuery);